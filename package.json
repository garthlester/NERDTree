{
  "name": "nerdtreeimp",
  "displayName": "NERDTree Improved",
  "description": "Better NERDTree for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "garthlester",
  "keywords": [
    "NERDTree",
    "vim",
    "vi",
    "vscodevim",
    "NERD",
    "tree",
    "explorer",
    "netrw"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/garthlester/NERDTree"
  },
  "homepage": "https://github.com/garthlester/NERDTree",
  "bugs": {
    "url": "https://github.com/garthlester/NERDTree/issues"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "activationEvents": [
    "onCommand:nerdtree.unfocusSidebarOrClose",
    "onCommand:nerdtree.openFile",
    "onCommand:nerdtree.openFileNewWindow"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "NERDTree Improved",
      "properties": {
        "nerdtree.suppressNotImplementedErrors": {
          "type": "boolean",
          "default": true,
          "description": "Prevents getting a popup if you try to use functionality that is not yet implemented"
        }
      }
    },
    "keybindings": [
      {
        "key": "ctrl+n",
        "command": "nerdtree.close",
        "when": "sideBarVisible && filesExplorerFocus"
      },
      {
        "key": "ctrl+n",
        "command": "workbench.files.action.focusFilesExplorer",
        "when": "sideBarVisible && !filesExplorerFocus && !suggestWidgetVisible && vim.mode != 'Insert'"
      },
      {
        "key": "ctrl+n",
        "command": "workbench.files.action.focusFilesExplorer",
        "when": "!sideBarVisible && !suggestWidgetVisible && vim.mode != 'Insert'"
      },
      {
        "key": "ctrl+n",
        "command": "editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible &&  vim.mode == 'Insert'"
      },
      {
        "key": "ctrl+n",
        "command": "selectNextSuggestion",
        "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus &&  vim.mode == 'Insert'"
      },
      {
        "key": "enter",
        "command": "nerdtree.openFile",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
      },
      {
        "key": "o",
        "command": "-list.toggleExpand"
      },
      {
        "key": "o",
        "command": "nerdtree.openFile",
        "when": "foldersViewVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
      },
      {
        "key": "g o",
        "command": "nerdtree.previewFile",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
      },
      {
        "key": "s",
        "command": "nerdtree.openVSplit",
        "when": "foldersViewVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
      },
      {
        "key": "g s",
        "command": "nerdtree.previewVSplit",
        "when": "foldersViewVisible && filesExplorerFocus && !explorerResourceIsFolder && !inputFocus"
      },
      {
        "key": "enter",
        "command": "nerdtree.toggleNode",
        "when": "explorerViewletVisible && filesExplorerFocus && explorerResourceIsFolder && !inputFocus"
      },
      {
        "key": "x",
        "command": "nerdtree.closeParentOfNode",
        "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m a",
        "command": "nerdtree.menu.add",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m m",
        "command": "nerdtree.menu.move",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m d",
        "command": "nerdtree.menu.delete",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m r",
        "command": "nerdtree.menu.reveal",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m o",
        "command": "nerdtree.menu.open",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m c",
        "command": "nerdtree.menu.copy",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m x",
        "command": "nerdtree.menu.cut",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m v",
        "command": "nerdtree.menu.paste",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m p",
        "command": "nerdtree.menu.copyPath",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m l",
        "command": "nerdtree.menu.list",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m Shift+C",
        "command": "nerdtree.menu.changePermissions",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "m s",
        "command": "nerdtree.menu.runSystemCommand",
        "when": "foldersViewVisible && filesExplorerFocus && !inputFocus"
      },
      {
        "key": "j",
        "command": "list.focusDown",
        "when": "listFocus && !inputFocus"
      },
      {
        "key": "k",
        "command": "list.focusUp",
        "when": "listFocus && !inputFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "vsce": "vsce package",
    "publish": "vsce publish",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
